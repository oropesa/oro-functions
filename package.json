{
  "name": "oro-functions",
  "version": "2.0.0",
  "description": "The Class Ofn extends Ofn-client and includes additional static helper functions for: URLs, Files, Operating System.",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "test": "jest",
    "lint": "eslint .",
    "clean": "rm -R ./dist",
    "clean:all": "rm -R ./dist ./node_modules",
    "build": "tsup ./src/index.ts --format cjs,esm --dts --clean"
  },
  "keywords": [
    "constant",
    "functions",
    "helpers",
    "typescript"
  ],
  "author": "Carlos Oropesa <carlos@oropensando.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oropesa/oro-functions.git"
  },
  "bugs": {
    "url": "https://github.com/oropesa/oro-functions/issues"
  },
  "homepage": "https://github.com/oropesa/oro-functions#readme",
  "dependencies": {
    "atob": "^2.1.2",
    "fast-glob": "^3.3.1",
    "fs-extra": "^11.1.1",
    "get-port-please": "^3.1.1",
    "oro-functions-client": "^2.1.0",
    "zip-lib": "^0.7.3"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-typescript": "^7.23.2",
    "@types/atob": "^2.1.3",
    "@types/crypto-js": "^4.1.3",
    "@types/express": "^4.17.20",
    "@types/fs-extra": "^11.0.3",
    "@types/jest": "^29.5.6",
    "@types/lodash.clonedeep": "^4.5.8",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.52.0",
    "eslint-config-alloy": "^5.1.2",
    "eslint-plugin-unicorn": "^48.0.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  }
